for y in range(10):
    if(y%2 ==0):
        for x in range(10):
            if(x%2 == 0):
                print("1", end=" ");
            else:
                print("0", end= " ");
        print("");
    else:
        for x in range(10):
            if(x%2 == 0):
                print("0", end = " ");
            else:
                print("1", end = " ");
        print()
